{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "production": false
    }
  },
  "env": {
    "VITE_API_URL": "http://ec2-3-66-218-103.eu-central-1.compute.amazonaws.com:4000",
    "VITE_SOCKET_URL": "ws://ec2-3-66-218-103.eu-central-1.compute.amazonaws.com:4000",
    "VITE_BASE_API_URL": "http://ec2-3-66-218-103.eu-central-1.compute.amazonaws.com:4000/api"
  }
}
